# platform :ios, '9.0'

target 'HackathonFiesp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'SDWebImage' # Async image downloader with cache support
  pod 'SVProgressHUD', '~> 2.1.2' # Loading overlay
  pod 'IQKeyboardManagerSwift', '4.0.10' # Keyboard Pod
  pod 'Alamofire', '~> 4.5' #URL connection. Request manager
  pod 'Tailor', '~> 2.2.1' #JSON object mapper
  pod 'Firebase/Core'
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'OneSignal', '>= 2.5.4', '< 3.0' # Push Notifications Pod

  # Pods for HackathonFiesp

end

target 'OneSignalNotificationServiceExtension' do
    use_frameworks!
    pod 'OneSignal', '>= 2.5.4', '< 3.0' # Push Notifications
end
